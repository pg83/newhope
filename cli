#!/usr/bin/env python
# replace me

import os
import sys

sys.dont_write_bytecode = True


try:
   from upm_cli import run_main
   tool = 'upm'
except ImportError:
   sys.path.append(os.path.dirname(os.path.abspath(__file__)))

   from upm.cli import run_main
   tool = os.path.abspath(__file__)

if __name__ == '__main__':
   run_main(tool=tool)
