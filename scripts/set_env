export PATH="$PATH:/bin:/usr/bin:/usr/local/bin"

export YSHELL=`which dash || which $OUTER_SHELL || which bash`
export YMAKE=`which gmake || which make`
export YXZ=`which xz`
export YTAR=`which bsdtar || which tar`
export YGZIP=`which gzip`
export YBZIP2=`which bzip2`
export YUNZIP=`which unzip`
export YCURL=`which curl`
export YWGET=`which wget`
export SHELL="$YSHELL"

export CFLAGS="-O2 -w"
