source set_env

rmrf () {
    rmdel "$1"
}

mkcd() {
    mkdir -p "$1"
    cd "$1"
}

rmmkcd() {
    rmrf "$1"
    mkcd "$1"
}

add_path() {
    while [ $# -gt 0 ]
    do
        export PATH="$1":$PATH
        shift
    done
}

add_path_1() {
    export PATH="$1":$PATH
}

last_log() {
    name=$(basename "$1")
    echo " $name:" && tail -n 50 "$1" | while read line; do echo "  $line"; done
}

rmdel() {
    (rm -rf $@) || true >& /dev/null

    return 0
}

fetch_url() {
    cd $3 && ((which wget && wget -O $1 $2) || (which curl && curl -L -k -o $1 $2)) && ls -la
}
