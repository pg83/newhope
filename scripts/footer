cd $IDIR && (find . | grep '\.a' | while read line; do
     llvm-ar d $line libc.a
     llvm-ranlib $line
done)

for i in "$BDIR/runtime/stdout.log" "$BDIR/runtime/stderr.log" "$BD/config.log"; do
    source copy_log "$i" "$IDIR/log"   
done

ext=$(source file_ext "$PKGF")

source rmdel "$IDIR/lib/"*.la
source prepare_"$ext"_pkg "$IDIR" "$PKGF"
