source main.lib

copy_log() {
    (mkdir -p "$2") && (touch "$1" >& /dev/null) && (cp "$1" "$2")

    return 0
}

copy_logs1() {
    if [ $# == 2 ]
    then
        copy_log $1 $2

        return 0
    fi

    f=$1

    shift
    shift

    copy_logs1 $f $@

    return 0
}

copy_logs() {
    while [ $# -gt 1 ]
    do
       copy_logs1 $@
       shift
    done

    return 0
}

copy_logs $@
