workspace: package_dir work_dir install_dir cache_dir source_dir release_upm 
install_dir: 
	export PATH=/bin:/sbin:/usr/bin:/usr/sbin:/usr/local/bin:/usr/local/sbin
	rm -rf "$PD" 2> /dev/null || true
	mkdir -p "$PD"


work_dir: 
	export PATH=/bin:/sbin:/usr/bin:/usr/sbin:/usr/local/bin:/usr/local/sbin
	rm -rf "$WD" 2> /dev/null || true
	mkdir -p "$WD"


package_dir: 
	export PATH=/bin:/sbin:/usr/bin:/usr/sbin:/usr/local/bin:/usr/local/sbin
	mkdir -p "$RD"


cache_dir: 
	export PATH=/bin:/sbin:/usr/bin:/usr/sbin:/usr/local/bin:/usr/local/sbin
	mkdir -p "$MD"


source_dir: 
	export PATH=/bin:/sbin:/usr/bin:/usr/sbin:/usr/local/bin:/usr/local/sbin
	mkdir -p "$SD"
	(rm -rf $SD/upm || true) 2> /dev/null


release_upm: source_dir 
	export PATH=/bin:/sbin:/usr/bin:/usr/sbin:/usr/local/bin:/usr/local/sbin
	($UPM cmd release > $SD/upm.tmp) && chmod +x $SD/upm.tmp && $SD/upm.tmp help && mv $SD/upm.tmp $SD/upm


